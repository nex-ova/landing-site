#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules/eslint/bin/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules/eslint/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules/eslint/bin/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules/eslint/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/eslint@9.31.0/node_modules:/home/sudoajay/Downloads/57952004-madula-health-medical-react-template/React-Madula-v1.0-06_May_2025/package/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@9.31.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@9.31.0/node_modules/eslint/bin/eslint.js" "$@"
fi
